<h1>All Tags: </h1>
<ul id="tags">
  <% @tags.each do |tag| %>
    <li><strong><%= link_to tag.name, tag_path(tag) %></strong>          <%= link_to "delete", tag_path(tag),
                          method: :delete,
                          data: {confirm: "Really delete the article?"} %></li>
  <% end %>
</tags>

<%= link_to "<< Back to Articles List", articles_path %>
